{
  "name": "ap-thread-reader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "lingui compile && vite build",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "deploy": "yarn build && flyctl deploy",
    "cli": "tsx scripts/cli.ts",
    "fmt": "oxfmt",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ && oxlint src/",
    "lint:fix": "eslint src/ --fix && oxlint src/ --fix",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile"
  },
  "dependencies": {
    "@fedify/fedify": "^1.3.0",
    "@js-temporal/polyfill": "^0.5.1",
    "@lingui/core": "^5.0.0",
    "@lingui/macro": "^5.9.0",
    "@lingui/react": "^5.0.0",
    "@logtape/logtape": "^0.8.0",
    "@optique/core": "^0.9.1",
    "@optique/run": "^0.9.1",
    "@tanstack/react-router": "^1.92.0",
    "@tanstack/react-start": "^1.157.0",
    "dompurify": "^3.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@lingui/babel-plugin-lingui-macro": "^5.9.0",
    "@lingui/cli": "^5.0.0",
    "@lingui/vite-plugin": "^5.0.0",
    "@types/babel-plugin-macros": "^3",
    "@types/babel__core": "^7",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^9.17.0",
    "jsdom": "^25.0.0",
    "nitro": "^3.0.1-alpha.2",
    "oxfmt": "^0.26.0",
    "oxlint": "^0.15.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^2.1.0"
  },
  "packageManager": "yarn@4.12.0"
}
